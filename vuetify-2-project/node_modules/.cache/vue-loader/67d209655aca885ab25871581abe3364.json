er({user:this.user,id:this.user.id}).then(res=>{\n        this.closeDialog()\n        this.$emit('userUpdated')\n      }).catch(error=> console.log(error)\n      )\n\n    }\n  }\n};\n</script>\n<style scoped>\n</style>"]}]}