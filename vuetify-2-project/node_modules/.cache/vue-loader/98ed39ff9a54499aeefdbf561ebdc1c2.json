;\n        \n  usuarioRepo\n      .addUser(user)\n      .then(res => {\n        this.closeDialog();\n        this.$emit('userAdded')\n      })\n      .catch(console.error);\n\n      }\n    },\n    closeDialog() {\n      this.$emit(\"dialogClose\");\n    }\n  }\n};\n</script>\n<style scoped>\n</style>"]}]}